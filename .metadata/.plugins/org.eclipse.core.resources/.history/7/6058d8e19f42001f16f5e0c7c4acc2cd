package com.datatypes;

public class SwitchCaseDemo {

	public static void main(String[] args) {

		String menuItem = "tea";
		
		int choice = 1;

		switch (choice) {
		case 1:

			System.out.println("You order Tea");
			break;
		case "coffee":

			System.out.println("You order Coffee");
			break;
				
		case "milk":

			System.out.println("You order Milk");

			break;

		default:

			System.out.println("Invalid Order");

			break;
		}

	}

}
